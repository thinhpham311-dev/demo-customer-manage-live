generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id        Int      @id @default(autoincrement())
  name      String?
  product   String?
  email     String   @unique
  id_client String?
  active    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
